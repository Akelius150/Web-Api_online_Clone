@model List<UserRefferalTableModel>

@{
    ViewData["Title"] = "Stats";
}

<div class="row">
    
    <table id="RefferalsUsers" class="table table-striped table-centered mb-0">
        <thead>
        <tr class="table-primary">
            <th>Email</th>
            <th>FullName</th>
            <th>RegistrationDate</th>
            <th>UserName</th>
            <th>ReffererId</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Email
                </td>
                <td>
                    @item.FullName
                </td>
                <td>
                    @item.RegistrationDate
                </td>
                <td>
                    @item.UserName
                </td>
                <td>
                    @item.ReffererId
                </td>
            </tr>
        }
        </tbody>
    </table>

</div>