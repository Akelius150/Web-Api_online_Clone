@model List<OutcomeTransactionTableModel>
@{
    ViewData["Title"] = "Stats";
}

<div class="row">
    
    <table id="OutcomeTransactionRepository" class="table table-striped table-centered mb-0">
    <thead>
        <tr class="table-primary">
            <th>FromWalletId</th>
            <th>ToAddress</th>
            <th>Value</th>
            <th>CreateDate</th>
            <th>CurrencyAcronim</th>
            <th>State</th>
            <th>LastUpdateDate</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.FromWalletId
                </td>
                <td>
                    @item.ToAddress
                </td>
                <td>
                    @item.Value
                </td>
                <td>
                    @item.CreateDate
                </td>
                <td>
                    @item.CurrencyAcronim
                </td>
                <td>
                    @item.State
                </td>
                <td>
                    @item.LastUpdateDate
                </td>

            </tr>
        }
    </tbody>
</table>

</div>