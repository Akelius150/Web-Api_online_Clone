@model List<TransferTableModel>
@{
    ViewData["Title"] = "Stats";
}

<div class="row">
    
    <table id="TransferRepository" class="table table-striped table-centered mb-0">
    <thead>
        <tr class="table-primary">
            <th>WalletFromId</th>
            <th>WalletToId</th>
            <th>Value</th>
            <th>Date</th>
            <th>CurrencyAcronim</th>
            <th>Hash</th>
            <th>Comment</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.WalletFromId
                </td>
                <td>
                    @item.WalletToId
                </td>
                <td>
                    @item.Value
                </td>
                <td>
                    @item.Date
                </td>
                <td>
                    @item.CurrencyAcronim
                </td>
                <td>
                    @item.Hash
                </td>
                <td>
                    @item.Comment
                </td>

            </tr>
        }
    </tbody>
</table>

</div>